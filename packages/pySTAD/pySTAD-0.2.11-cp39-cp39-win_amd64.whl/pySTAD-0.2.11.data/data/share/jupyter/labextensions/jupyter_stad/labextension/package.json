{
  "name": "jupyter_stad",
  "version": "0.2.5",
  "description": "A Jupyter widget for interactive network visualization",
  "author": {
    "name": "Jelmer Bot",
    "email": "jelmer.bot+gitlab@uhasselt.be"
  },
  "homepage": "https://gitlab.com/dsi_uhasselt/vda-lab/pystad2",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/dsi_uhasselt/vda-lab/pystad2.git"
  },
  "bugs": {
    "url": "https://gitlab.com/dsi_uhasselt/vda-lab/pystad2/-/issues"
  },
  "license": "BSD-3-Clause",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets",
    "pySTAD"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "npm run clean:lib && npm run clean:labextension && npm run clean:nbextension",
    "clean:lib": "rimraf lib/",
    "clean:labextension": "rimraf ../stad/widget/labextension/",
    "clean:nbextension": "rimraf ../stad/widget/nbextension/",
    "build": "webpack --mode=production && jupyter labextension build . && cp extension.js ../stad/widget/nbextension/",
    "build:dev": "webpack --mode=development && jupyter labextension build --development True . && cp extension.js ../stad/widget/nbextension/",
    "watch": "webpack --watch --mode=development",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,svelte} && prettier --check src",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx,svelte} --fix && prettier --write src"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.9",
    "@phosphor/application": "^1.7.3",
    "@phosphor/widgets": "^1.9.3",
    "@tsconfig/svelte": "^1.0.13",
    "@types/node": "^15.14.9",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.2",
    "style-loader": "^2.0.0",
    "svelte": "^3.42.3",
    "svelte-loader": "^3.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@bulatdashiev/svelte-slider": "^1.0.3",
    "@jupyter-widgets/base": "^3 || ^4",
    "d3-array": "^2.12.1",
    "d3-axis": "^2.1.0",
    "d3-color": "^3.1.0",
    "d3-force": "^2.1.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^2.0.0",
    "html2canvas": "^1.3.2",
    "lodash": "^4.17.21",
    "pixi-viewport": "^4.32.0",
    "pixi.js": "^6.1.2",
    "robust-point-in-polygon": "^1.0.3"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "../stad/widget/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    },
    "_build": {
      "load": "static\\remoteEntry.09ad0fb12332fec69d43.js",
      "extension": "./extension"
    }
  }
}
