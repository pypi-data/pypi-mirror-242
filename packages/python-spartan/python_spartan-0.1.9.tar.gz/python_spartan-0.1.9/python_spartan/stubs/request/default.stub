from pydantic import BaseModel
from datetime import datetime

class {{name}}Request(BaseModel):
    id: int
    created_at: datetime
    updated_at: datetime

    class Config:
        schema_extra = {
            "example": {
                    "id": 1,
                    "created_at": "2023-12-12 00:00:00",
                    "updated_at": "2023-12-12 00:00:00"
                }
        }
